<script type="text/javascript">
	var switch_flag = false;
	function expand(){
		$('#sidebar').toggleClass('hide');
		$('.navbar-container.content').toggleClass('expand');
		$('.app-content.content').toggleClass('expand');
		$('footer.footer').toggleClass('expand');
		setTimeout(() => {
		$('#viewer_3d').width('calc(100%)');
		}, 100);
	}
	function switchList(){
		switch_flag  = !switch_flag;
		if(switch_flag){
			$(".chart_element").removeClass('col-lg-6').addClass('col-lg-12');
			$(".la.la-expand").removeClass('la-expand').addClass('la-compress');
		}
		else{
			$(".chart_element").removeClass('col-lg-12').addClass('col-lg-6');
			$(".la.la-compress").removeClass('la-compress').addClass('la-expand');
		}
		
	}
</script>
